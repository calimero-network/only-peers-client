{
  "name": "only-peers-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "concurrently \"pnpm:lint:*(!fix)\"",
    "lint:src": "eslint . --ext .ts,.tsx src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\"",
    "lint:src:fix": "eslint . --ext .ts,.tsx --fix src",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d build",
    "prettier": "exec prettier . --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@calimero-network/calimero-client": "1.6.4",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@near-wallet-selector/account-export": "^8.9.5",
    "@near-wallet-selector/core": "^8.9.5",
    "@near-wallet-selector/modal-ui": "^8.9.5",
    "@near-wallet-selector/my-near-wallet": "^8.9.5",
    "@near-wallet-selector/near-wallet": "^8.9.3",
    "@tailwindcss/postcss": "^4.0.17",
    "@tailwindcss/vite": "^4.0.17",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.4",
    "concurrently": "^7.3.0",
    "postcss": "^8.5.3",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^7.4.0",
    "tailwindcss": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.6"
  }
}
